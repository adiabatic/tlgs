{
	"app":{
		"document_root": "./contents",
		"file_types": [
			"gif",
			"png",
			"jpg",
			"js",
			"css",
			"html",
			"ico",
			"swf",
			"xap",
			"apk",
			"cur",
			"xml",
			"gmi",
			"txt"
		],
		"use_implicit_page": true,
		"implicit_page": "index.gmi",
		"home_page": "index.gmi",
		"mime": {
			"text/gemini": ["gmi", "gemini"]
		}
	},
	"plugins": [
		{
			"name": "dremini::GeminiServerPlugin",
			"config": {
				"listeners": [
					{
						"key": "../../key.pem",
						"cert": "../../cert.pem",
						"ip": "0.0.0.0",
						"port": 1966
					}
				],
				"numThread": 3,
				"translate_to_html": true
			}
		},
		{
			"name": "spartoi::SpartanServerPlugin",
			"config": {
				"listeners": [
					{
						"ip": "0.0.0.0",
						"port": 3000
					}
				],
				"numThread": 3,
				"translate_to_html": true
			}
		}
	],
	"listeners": [
		{
			"ip": "0.0.0.0",
			"port": 8808
		}
	],
	"db_clients": [
		{
			"rdbms": "postgresql",
			"host": "127.0.0.1",
			"port": 5432,
			"dbname": "tlgs",
			"number_of_connections": 4,
			"timeout": 10
		}
	 ],
	 "custom_config": {
		 "tlgs": {
			 "ranking_algo": "salsa"
		 }
	 }
}
